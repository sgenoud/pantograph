{
  "name": "pantograph2d",
  "version": "0.7.0",
  "description": "Pantograph, the pure JS 2D CAD library",
  "type": "module",
  "main": "./dist/pantograph.cjs",
  "module": "./dist/pantograph.js",
  "files": [
    "dist"
  ],
  "types": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/pantograph.js",
        "types": "./dist/types/main.d.ts"
      },
      "require": {
        "default": "./dist/pantograph.cjs",
        "types": "./dist/types/main.d.ts"
      }
    },
    "./models": {
      "import": {
        "default": "./dist/pantograph/models.js",
        "types": "./dist/types/api/models.d.ts"
      },
      "require": {
        "default": "./dist/pantograph/models.cjs",
        "types": "./dist/types/api/drawShape.d.ts"
      }
    },
    "./drawShape": {
      "import": {
        "default": "./dist/pantograph/drawShape.js",
        "types": "./dist/types/api/drawShape.d.ts"
      },
      "require": {
        "default": "./dist/pantograph/drawShape.cjs"
      }
    }
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest"
  },
  "keywords": [
    "2d",
    "graphics",
    "cad",
    "svg"
  ],
  "author": "Steve Genoud",
  "license": "AGPL",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.35.1",
    "@types/node": "^18.15.5",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "prettier": "^2.8.4",
    "terminal-image": "^2.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.29.2",
    "xmldom": "^0.6.0"
  },
  "gitHead": "a2b9db5c938cdbd7452e96175b403fac8f4caf5b",
  "dependencies": {
    "-": "^0.0.1",
    "@thi.ng/adjacency": "^2.5.51",
    "@thi.ng/associative": "^6.3.60",
    "@thi.ng/equiv": "^2.1.58",
    "@thi.ng/geom-accel": "^3.6.4",
    "@types/flatbush": "^3.3.0",
    "flatbush": "^4.1.0",
    "ml-matrix": "^6.10.4",
    "path-data-parser": "^0.1.0"
  }
}
